doctype html
html
  head
    title TableView;
    //- link(rel="stylesheet" href="style.css" type='text/css')
  body
    h1 Register
    form(action='/register', method='post')
        p Username 
          input(type='text', id='nameID', name='registUser' required)
        p Email 
          input(type='text', id='email', name='registEmail' required)
        p Password 
          input(type='text', id='passwordID', name='registPassword' required)
        button(type='submit') Register
    a(href='/login') Login